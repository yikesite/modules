<!-- Slick carousel module dependencies -->

<!-- Make sure jQuery is declared before any slick.js -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

<!-- FontAwesome is used for the previous/next arrows. -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">

<link rel="stylesheet" href="/stylesheets/slick.css">
<link rel="stylesheet" href="/stylesheets/slick-theme.css">
<script src="/javascripts/slick.js"></script>

<script type="text/javascript">

  $(document).ready(function(){

    // See http://kenwheeler.github.io/slick/ for all configurable settings.
    $('.slick-carousel').slick({
      infinite: true,
      slidesToShow: 4,
      slidesToScroll: 1
    });
    
  });​

</script>
